module pdef.test.messages;


/** Enum. */
enum TestEnum {
    ONE, TWO, THREE;
}


/** Simple message . */
@form
message SimpleMessage {
    aString     string;
    aBool       bool;
    anInt16     int16;
}

/** Message with fields of all data types. */
message ComplexMessage : SimpleMessage {
    anInt32     int32;
    anInt64     int64;
    aFloat      float;
    aDouble     double;

    aList       list<int32>;
    aSet        set<int32>;
    aMap        map<int32, float>;

    anEnum      TestEnum;
    aMessage    ComplexMessage;
    anObject    object;
}
