module pdef.fixtures.interfaces;
import pdef.fixtures as fixtures;


interface Collection<Id, T> {
    get(id Id)  T;
}

interface User {
    name() string;
    rename(name string) string;
}

interface List<Id, T> {
    find(ids list<Id>) list<T>;
}

interface Users extends Collection<int32, User>, List<int32, fixtures.User> {
    register(
        name        string,
        email       string,
        birthday    Date) User;
}

message Date {
    year    int32;
    month   int32;
    day     int32;
}
