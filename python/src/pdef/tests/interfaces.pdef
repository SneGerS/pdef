package test.interfaces;


message UserData {
    string id;
    string name;
    List<PhotoData> photos;
}

message PhotoData {
    string id;
    string description;
    string url;

    UserData user;
}

interface App {
    photos() Photos;
}

interface Users {
    get(id string) User;
    register(email string, password string) User;
}

interface User {
    data() UserData;
}

interface Photos {
    get(id string) Photo;
    create(description string, url string) Photo;
}

interface Photo {
    data() PhotoData;
    comments() List<string>;
}
