package com.zumla.test;
import http.responses as http;


enum Type {
    OBJECT,
    USER,
    PHOTO,
    COMMENT,
    EVENT,
    LIKE;
}

message MyResponse extends http.HttpResponse {

}

message Id {
    options [type_field: "type"];
    type Type [value: "Type.OBJECT"];
}

message Entity {
    type Type;
}

message CommentId extends Id {
    type    Type [value: "Type.COMMENT"];
    value   int64;
}

message UserId extends Id {
    type    Type [value: "Type.USER"];
    value   int64;
}

message User extends Entity {
    name    string;
    age     int64;
    profile Profile;
}

message Profile {
    firstName   string;
    lastName    string;
    aliases     List<string>;
}

enum Sex {
    MALE,
    FEMALE;
}

enum EventType {
    GENERIC;
}

message EventId extends Id {
    type Type [value: "Type.EVENT"];
}
